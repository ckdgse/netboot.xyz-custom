
#!ipxe
###
### {{ site_name }} StVital.io
###

:custom
clear custom_choice
menu StV
item --gap Load ISO
item option_two ${space} Loading an ISO
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:option_two
kernel {{ memdisk_location }} raw iso
read isopath
initrd ${isopath}
boot || goto custom_exit

:custom_exit
sleep 10
exit 0
